{
    "name": "mirvan/ippanel",
    "description": "laravel notification channel for ippanel",
    "license": "MIT",
    "authors": [
        {
            "name": "MirVan",
            "email": "me@mirvan.ir",
            "homepage": "https://mirvan.ir"
        }
    ],
    "homepage": "https://github.com/mirvangh/ippanel",
    "keywords": ["Laravel", "Laravel Notification Channel","IPPanel"],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "5.5 - 10",
        "illuminate/support": "5.5 - 10",
        "ippanel/php-rest-sdk": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^7.5 || ^8.5.21 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mirvan\\IPPanel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mirvan\\IPPanel\\IPPanelServiceProvider"
            ],
            "aliases": {
                "IPPanel": "Mirvan\\IPPanel\\Facades\\IPPanel"
            }
        }
    }
}
