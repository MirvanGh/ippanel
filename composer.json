{
    "name": "mirvan/ippanel",
    "description": "laravel notification channel for ippanel",
    "license": "MIT",
    "authors": [
        {
            "name": "MirVan",
            "email": "me@mirvan.ir",
            "homepage": "https://mirvan.ir"
        }
    ],
    "homepage": "https://github.com/mirvangh/ippanel",
    "keywords": ["Laravel", "Laravel Notification Channel","IPPanel"],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "ippanel/php-rest-sdk": "^1.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mirvan\\IPPanel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mirvan\\IPPanel\\IPPanelServiceProvider"
            ],
            "aliases": {
                "IPPanel": "Mirvan\\IPPanel\\Facades\\IPPanel"
            }
        }
    }
}
